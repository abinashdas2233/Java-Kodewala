package com.kodewala.learn;

public class Payment {
	int amount=2000;
	String password;
	public Payment(String password) {
		this.password=password;
	}
	public Payment() {
		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Payment abhi=new Payment("123456@");
		System.out.println("Password set success fully. password="+abhi.password);
		System.out.prinln("amount="+abhi.amount);

	}

}
